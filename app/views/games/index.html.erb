<h2 class="center">Games</h2>
<hr />
<div class="row">
  <div class="col s12 m6">
    <button id="toggle" class="btn">Show Form</button>
    <ul id="games-list">
      <% @games.each do |game|  %>
        <li
          class="game-item"
          data-id=<%= game.id %>
          data-name="<%= game.name %>"
        >
          <%= game.name %> - <%= game.description %>
        </li>
    <% end %>
    </ul>
  </div>
  <div id="characters-list" class="col s12 m6">
    <h4 id="game">Characters in Game</h4>
    <ul id="characters">
    </ul>
  </div>
</div>
