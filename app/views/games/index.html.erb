<h2 class="center">Games</h2>
<hr />
<div class="row">
  <div class="col s12 m6">
    <ul id="idgames-list">
      <% @games.each do |game|  %>
        <li
          class="game-item"
          data-id=<%= game.id %>
        >
          <%= game.name %> - <%= game.description %>
        </li>
    <% end %>
    </ul>
  </div>
  <div id="characters-list" class="row">
    <h3 id="game">Characters in Game</h3>
    <ul id="characters">
    </ul>
  </div>
</div>
